                <div class="wrap">

                    <div class="intro">
                        <p><h1>Responsive JQuery Popup</h1></p>	
                        <p><i>"Full responsive popup with JQuery using a button to hide it."</i></p><br>
                        <!-- Popup action -->
                        <a onClick="openPopup('#wrap_popup1')">Open 1st Popup</a> | 
                        <a onClick="openPopup('#wrap_popup2')">Open 2nd Popup</a>
                    </div>

                    <!-- Popup -->
                    <div id="wrap_popup1" class="wrap_popup">
                        <div class="popup">
                            <div class="title">
                                <p><i class="fas fa-phone-square-alt"></i> DIAL IN LINE<i class="fas fa-headset"></i> <br>+256-702-932-565</p>
                            </div>
                            <div class="box">
                                <p>
                                <form name="mydata" action="/booking/booking"  method="POST">
                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="names">Names</label>
                                        <input type="text" class="form-control" id="names" name="names">
                                        <div class="error" id="names_err"></div>
                                    </div>
                                    <div class="col-md-6 mb-3">
                                        <label for="email">Email</label>
                                        <input type="email" class="form-control" id="email" name="email">
                                        <div class="error" id="email_err"></div>
                                    </div>

                                </div>

                                <div class="form-row">
                                    <div class="col-md-12 mb-3">
                                        <label for="date">Check In</label>
                                        <input type="date" class="form-control" id="date" name="date">
                                        <div class="error" id="date_err"></div>
                                    </div>
                                </div>

                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="product">Product Name</label>
                                        <input type="text" class="form-control" id="product" name="product">
                                        <div class="error" id="product_err"></div>
                                    </div>
                                        <div class="col-md-6 mb-3">
                                        <label for="quantity">Quantity</label>
                                        <input type="text" class="form-control" id="quantity" name="quantity">
                                        <div class="error" id="quantity_err"></div>
                                    </div>
                                </div>

                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="payment">Mode Of Payment</label>
                                        <select class="custom-select" id="payment" name="payment">
                                            <option selected value="-- Mode Of Payment --">-- Mode Of Payment --</option>
                                            <option value="Cash">Cash</option>
                                            <option value="Mobile Money">Mobile Money</option>
                                        </select>
                                        <div class="error" id="payment_err"></div>
                                    </div>
                                    <div class="col-md-6 mb-3">
                                        <label for="delivery">Mode Of Delivery</label>
                                        <select class="custom-select" id="delivery" name="delivery">
                                            <option selected value="-- Mode Of Delivery --">-- Mode Of Delivery --</option>
                                            <option value="Pickup">Pickup</option>
                                            <option value="Home Delivery">Home Delivery</option>
                                        </select>
                                        <div class="error" id="delivery_err"></div>
                                    </div>
                                </div>

                                <div class="form-row">
                                    <div class="col-md-6 mb-3">
                                        <label for="phoneNumber">Phone Number.</label>
                                        <input type="text" class="form-control" id="phoneNumber" name="phoneNumber">
                                        <div class="error" id="phoneNumber_err"></div>
                                    </div>
                                    <div class="col-md-6 mb-3">
                                        <label for="directions">Location</label>
                                        <input type="text" class="form-control" id="directions" name="directions">
                                        <div class="error" id="directions_err"></div>
                                    </div>
                                </div>

                            <center><button class="btn btn-primary" type="submit" onclick='officeRegistration()'>Order</button></center>
                            </form>
                                </p>
                                <center><button class="btn btn-secondary" style=color="red" onClick="closePopup('#wrap_popup1')">Close Popup</button></center>
                            </div>
                        </div>
                    </div>
                    <!-- End Popup -->
